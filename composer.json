{
    "name": "adlogix/guzzle-atlassian-connect-middleware",
    "description": "An Atlassian Connect authentication middleware for Guzzle",
    "version": "0.1.2",
    "license": "MIT",
    "keywords": [
        "Atlassian",
        "Connect",
        "JWT",
        "QSH",
        "Guzzle",
        "Confluence",
        "JIRA",
        "Authentication",
        "Middleware"
    ],
    "authors": [
        {
            "name": "Cedric Michaux",
            "email": "cedric@adlogix.eu",
            "homepage": "http://www.adlogix.eu"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Adlogix\\GuzzleAtlassianConnect\\": [
                "src/",
                "tests/"
            ]
        }
    },
    "require": {
        "php": ">=5.5.0",
        "firebase/php-jwt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "guzzlehttp/guzzle": "^6.0",
        "silex/silex": "^1.3"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For using this library as it was created for Guzzle6. But you should be able to use it with any PSR-7 HTTP Client"
    }
}
